{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "name": {
      "value": "${AZURE_ENV_NAME}"
    },
    "location": {
      "value": "${AZURE_LOCATION}"
    },
    "principalId": {
      "value": "${AZURE_PRINCIPAL_ID}"
    },
    "serviceManagementReference": {
      "value": "${AZURE_SERVICE_MANAGEMENT_REFERENCE}"
    },
    "acaExists": {
      "value": "${SERVICE_ACA_RESOURCE_EXISTS=false}"
    },
    "includeTokenStore": {
      "value": "${USE_ACA_TOKEN_STORE=false}"
    },
    "customHostName": {
      "value": "${CUSTOM_HOST_NAME}"
    },
    "customCertificateName": {
      "value": "${CUSTOM_CERTIFICATE_NAME}"
    },
    "sharePointBaseUrl": {
      "value": "${VITE_SHAREPOINT_BASE_URL}"
    },
    "dataFabricApiUrl": {
      "value": "${DATA_FABRIC_API_URL}"
    },
    "dataFabricApiKey": {
      "value": "${DATA_FABRIC_API_KEY}"
    },
    "webhookBatchCompleteKey": {
      "value": "${WEBHOOK_BATCH_COMPLETE_KEY}"
    },
    "uipathTenantName": {
      "value": "${UIPATH_TENANT_NAME}"
    },
    "uipathAppId": {
      "value": "${UIPATH_APP_ID}"
    },
    "uipathApiKey": {
      "value": "${UIPATH_API_KEY}"
    },
    "uipathFolderId": {
      "value": "${UIPATH_FOLDER_ID}"
    },
    "uipathQueueName": {
      "value": "${UIPATH_QUEUE_NAME}"
    },
    "cosmosDatabaseName": {
      "value": "${COSMOS_DATABASE_NAME=kapitol-tender-automation}"
    },
    "cosmosMetadataContainerName": {
      "value": "${COSMOS_METADATA_CONTAINER_NAME=metadata}"
    },
    "cosmosBatchReferenceContainerName": {
      "value": "${COSMOS_BATCH_REFERENCE_CONTAINER_NAME=batch-reference-index}"
    },
    "metadataStoreMode": {
      "value": "${METADATA_STORE_MODE=cosmos}"
    },
    "metadataReadFallback": {
      "value": "${METADATA_READ_FALLBACK=true}"
    }
  }
}
